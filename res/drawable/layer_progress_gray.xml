<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@*android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffadadad" android:endColor="#ffadadad" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffadadad" />
            <stroke android:width="1.0px" android:color="#ffadadad" />
            <solid android:color="@*android:color/white" />
        </shape>
    </item>
    <item android:id="@*android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#80adadad" android:endColor="#a0adadad" android:angle="270.0" android:centerY="0.75" android:centerColor="#80adadad" />
            </shape>
        </clip>
    </item>
    <item android:id="@*android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <stroke android:width="5.0px" android:color="@*android:color/transparent" />
                <solid android:color="#ffadadad" />
            </shape>
        </clip>
    </item>
</layer-list>